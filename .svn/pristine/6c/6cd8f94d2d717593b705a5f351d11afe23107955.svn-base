using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms.DataVisualization.Charting;

namespace AverageModule.Model
{
    public class ChartModel
    {
        public string SeriesName { get; set; }
        public Series Serie { get; set; }
        public AxisParamStruct XAxisParams { get; set; }
        public AxisParamStruct YAxisParams { get; set; }
        public int POCZSR { get; set; }
        public int SK { get; set; }

        public ChartModel(string seriesName, Series serie, AxisParamStruct xAxisParams, AxisParamStruct yAxisParams, int poczsr, int sk)
        {
            SeriesName = seriesName;
            Serie = serie;
            XAxisParams = xAxisParams;
            YAxisParams = yAxisParams;
            POCZSR = poczsr;
            SK = sk;
        }
    }
}
